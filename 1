#include <iostream>
#include <vector>

template <typename Container>
void Print(const Container& container, const std::string& separator) {
    bool isFirst = true; 
    for (const auto& element : container) {
        if (!isFirst) {
            std::cout << separator;
        }
        std::cout << element;
        isFirst = false;
    }
    std::cout << '\n';
}

int main() {
    std::vector<int> data = {2, 4, 6};
    Print(data, ", ");
}
